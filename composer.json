{
    "name": "arcanedev/head",
    "description": "Head Helper",
    "keywords": ["title", "description", "charset", "meta", "style", "stylesheet", "script", "favicon", "google analytics", "laravel"],
    "homepage": "http://www.arcanedev.net",
    "license": "MIT",
    "authors": [
        {
            "name": "ARCANEDEV",
            "email": "arcanedev.maroc@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "arcanedev/markup": "~1.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpcov": "~2.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Head\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Head\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-mbstring": "To get encoding charset list."
    },
    "minimum-stability": "stable"
}
